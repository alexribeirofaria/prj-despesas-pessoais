{
  "name": "despesas-frontend-angular",
  "version": "2.0.0",
  "workspaces": [
    "app-android",
    "app-ios"
  ],
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "build": "ng build",
    "build:dev": "ng build --configuration=development",
    "watch": "ng build --watch --configuration development",
    "test": "set BROWSER=ChromeHeadless && ng test --browsers=ChromeHeadless --no-watch",
    "test:debug": "set BROWSER=Chrome && ng test --browsers=Chrome",
    "test:coverage": "ng test --code-coverage --browsers=ChromeHeadless --no-watch",
    "test:watch": "ng test --code-coverage --browsers=ChromeHeadless --watch",
    "prestart": "node aspnetcore-https",
    "start:windows": "ng serve --ssl --configuration local-development --ssl-cert \"%LOCALAPPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%LOCALAPPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng serve --ssl --configuration local-development --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1",
    "add:android": "npm run add:android --workspace=app-android",
    "add:ios": "npm run add:ios --workspace=app-ios",
    "install:android": "npm install --workspace=app-android",
    "install:ios": "npm install --workspace=app-ios",
    "build:android": "npm run build:mobile --workspace=app-android",
    "build:ios": "npm run build:mobile --workspace=app-ios",
    "start:android": "npm run start:android --workspace=app-android",
    "start:ios": "npm run start:ios --workspace=app-ios"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "19.2.19",
    "@angular/common": "^19.2.14",
    "@angular/compiler": "^19.2.14",
    "@angular/core": "^19.2.14",
    "@angular/forms": "^19.2.14",
    "@angular/material": "19.2.19",
    "@angular/material-moment-adapter": "19.2.19",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/router": "^19.2.14",
    "@date-io/dayjs": "^2.17.0",
    "@ng-bootstrap/ng-bootstrap": "19.0.1",
    "@popperjs/core": "^2.11.8",
    "angular-datatables": "^16.0.1",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "chart.js": "^4.4.3",
    "chartjs-plugin-datalabels": "^2.2.0",
    "datatables.net": "^1.13.11",
    "datatables.net-dt": "^1.13.11",
    "datatables.net-select": "^1.7.0",
    "datatables.net-select-dt": "^1.7.0",
    "dayjs": "^1.11.10",
    "google-auth-library": "^10.2.1",
    "jquery": "^3.7.1",
    "mdb-ui-kit": "^7.3.2",
    "moment": "^2.30.1",
    "ng2-charts": "^5.0.4",
    "ng2-currency-mask": "^13.0.3",
    "ngx-mask": "^16.4.2",
    "oidc-client": "^1.11.5",
    "run-script-os": "*",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.15",
    "@angular/localize": "^19.2.14",
    "@types/bootstrap": "^5.2.6",
    "@types/jasmine": "~5.1.0",
    "@types/jquery": "^3.5.32",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.55.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  }
}
